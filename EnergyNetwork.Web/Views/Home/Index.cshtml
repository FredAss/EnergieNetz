    </head>
    <body>
        <div class="st-sticky-wrap" id="applicationHost">
            <div class="splash well">
                <div class="message">
                    EnergieNetz
                    <p class="lead">Das Energiemanagementtool für EnergieEffizienz Netzwerke</p>
                </div>
            </div>
        </div>

        @Scripts.Render("~/scripts/vendor")
    
        @*<script type="text/javascript" src="~/App/main-built.js"></script>*@
        @if (HttpContext.Current.IsDebuggingEnabled)
        {
            <script type="text/javascript" src="~/bower_components/requirejs/require.js" data-main="@Url.Content("~/App/main")"></script>
        }
        else
        {
            <script type="text/javascript" src="~/App/main-built.js"></script>
        }

    </body>

</html>